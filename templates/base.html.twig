<!DOCTYPE html>
<html>
    <head>
        {% block head %}
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" href="{{ path('app_snippets_list') }}">Snippets</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('app_snippets_profile') }}">profile</a>
                    </li>
                </ul>
            </div>
        </nav>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="/bootstrap-4.1.3-dist/css/bootstrap.css">
            <link rel="stylesheet" href="/bootstrap-4.1.3-dist/css/style.css">
        {% endblock %}
    {% endblock %}
</head>
<body>
    {% block body %}{% endblock %}
    {% block javascripts %}
        <script src="/bootstrap-4.1.3-dist/js/jquery.min.js"></script>
        <script src="/bootstrap-4.1.3-dist/js/tether.min.js"></script>
        <script src="/bootstrap-4.1.3-dist/js/bootstrap.min.js"></script>
    {% endblock %}
</body>
</html>
